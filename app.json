{
  "expo": {
    "name": "Mall App",
    "slug": "mall-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "woomad",
    "userInterfaceStyle": "automatic",
    "sdkVersion": "52.0.0",
    "platforms": ["ios", "android", "web"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.woomad.app"
    },
    "android": {
      "package": "com.woomad.app"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-auth-session",
        {
          "scheme": "woomad"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "assetBundlePatterns": ["**/*"],
    "newArchEnabled": true
  }
}
